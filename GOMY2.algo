 // PROBLEM NUMBER 2 
VAR 
tab1:ARRAY_OF NUMBERS;
tab2:ARRAY_OF NUMBERS;
arr:empty array;
sum:INTEGER;
i:INTEGER;

BEGIN 
tab1:{3,1,7,9};
tab2:{2,4,1,9,3};
FOR i FROM 0 TO tab1.length DO 
IF (tab2.includes(tab1[i]) === false)
arr.push(tab1[i])
sum:sum+tab1[i];

END_IF 
END_FOR

FOR i FROM 0 TO tab2.length DO 
IF (tab1.includes(tab2[i]) === false)
arr.push(tab2[i])
sum:sum+tab2[i];

END_IF 
END_FOR

END 